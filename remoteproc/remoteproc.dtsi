/{
    /* Example for using reset driver dts entries for Ethos-U */
    ethosu_reset: sgm775_ethosu_bridge@0x500f0000 {
            compatible ="arm,ethosu-bridge-reset";
            #reset-cells = <1>;
            reg = <0x0 0x500f0000 0x0 0x00001000>;
    };

};

